//找环的入口点
#if 0
struct ListNode {
	int val;
	struct ListNode *next;
	ListNode(int x) :
		val(x), next(NULL) {
	}
};
ListNode* EntryNodeOfLoop(ListNode* pHead)
{
	if (pHead == NULL)
		return NULL;

	ListNode* first = pHead;
	ListNode* slow = pHead;
	while (first->next!=NULL&&slow!= NULL)
	{
		first = first->next->next;
		slow = slow->next;
		if (slow == first)
		{
			slow = pHead;
			while (slow != first)
			{
				first = first->next;
				slow = slow->next;
			}
			return slow;
		}
	}
	return NULL;
}
