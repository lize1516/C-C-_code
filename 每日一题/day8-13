//1.判断二叉树是否是对称二叉树

bool isSymmetrical(TreeNode* pRoot)
    {
          if(pRoot==NULL)
              return true;
         return isSymmetrical(pRoot->left,pRoot->right);
    }
    bool isSymmetrical(TreeNode* t1,TreeNode* t2)
    {
        if(t1==NULL&&t2==NULL)
            return true;
        if(t1==NULL||t2==NULL)
            return false;
        if(t1->val!=t2->val)
            return false;
        return isSymmetrical(t1->left,t2->right)&&isSymmetrical(t1->right,t2->left);
        
    }
