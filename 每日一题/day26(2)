 //two-sum    LeetCode
 
 vector<int> twoSum(vector<int> &numbers, int target) {
        vector<int> ret;
        int size=numbers.size();
        for(int i=0;i<size-1;i++)
        {
            int j=i+1;
            for(;j<size;j++)
            {
                if((numbers[j]+numbers[i])==target)
                {
                    ret.push_back(i+1);
                    ret.push_back(j+1);
                    return ret;
                }
            }
        }
        return ret;
    }
