//使用两个栈实现一个队列
#include<stack>
stack<int> stack1;
stack<int> stack2;
void push(int node) {
	stack1.push(node);
}

int pop() {
	int ret = 0;
	if (stack2.empty()&&(!stack1.empty()))
	{
		int i = 0;
		int size = stack1.size();
		for (i = 0; i < size; i++)
		{
			stack2.push(stack1.top());
			stack1.pop();
		}
		ret = stack2.top();
		stack2.pop();
	}
	else if (!stack2.empty())
	{
		ret = stack2.top();
		stack2.pop();
	}
	else if (stack1.empty() && stack2.empty())
	{
		return NULL;
	}
	return ret;
}

int main()
{
	
	system("pause");
	return 0;
}
