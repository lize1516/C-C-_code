//找重复数字
bool duplicate(int numbers[], int length, int* duplication) {
	
	int *arr = new int[length];
	memset(arr, 0, length*sizeof(int));
	for (int i = 0; i < length; i++)
	{
		arr[numbers[i]]++;
		if (arr[numbers[i]] == 2)
		{
			*duplication = numbers[i];
			return true;
		}
	}
	return false;
}
int main()
{

	int arr1[] = { 2, 1, 3, 1, 4 };
	int d = 0;
	duplicate(arr1, 5, &d);
	cout << d << endl;

	system("pause");
	return 0;
}
